{
    "manifest_version": 3,
    "name": "Word change Extension",
    "description": "Replaces all instances of the word 'hate' with 'love' ",
    "version": "1.0",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "permissions": ["activeTab", "declarativeContent", "scripting"],
    "background": {
      "service_worker": "service-worker.js"
    }
  }